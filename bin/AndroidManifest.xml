<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs_reversi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/reversiicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Difficulty"
            android:label="@string/title_activity_difficulty" >
        </activity>
        <activity
            android:name=".Gameboard"
            android:label="@string/title_activity_gameboard" >
        </activity>
        <activity
            android:name=".Highscores"
            android:label="@string/title_activity_highscores" >
        </activity>
    </application>

</manifest>
